define ->
  widget = (className, content) -> (additionalClasses) ->
    w = document.createElement('span')
    w.className = 'substitution ' + className
    if additionalClasses? then w.className += ' ' + additionalClasses
    w.className = 'cm-' + (w.className.replace /\ +/g, ' cm-')
    w.appendChild(document.createTextNode(content))
    return w
